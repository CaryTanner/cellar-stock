<div class="container-fluid col-md-6 justify-content-center">
    <h3 class="mt-3 ">{{title}}</h3>
    <div >{{#each varieties_list}}
        <ul class="list-group mt-3 ">
            <li class="list-group-item">
                <h3>{{this.varietyName}}</h3>
                <a href="/catalog/variety/{{this.varietyName}}"><button class="btn btn-outline-primary btn-sm mt-3">Wines</button></a>
                <a href="/catalog/variety/{{this._id}}/delete"><button
                        class="btn btn-outline-danger btn-sm mt-3">Delete</button></a>
                </li>
        </ul>


        {{/each}}
    </div>
</div>