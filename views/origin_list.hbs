<div class="container-fluid col-md-5 justify-content-center">
    <h3 class="mt-2">{{title}}</h3>
    <div>
        {{#each origin_list}}

        <ul class="list-group mt-2 ">
            <li class="list-group-item ">
                <h5 class="mt-0 mb-0">{{this.country}}</h5>
            
                <h3 class="mt-1 mb-0">{{this.region}}</h3>
           
                <a href="/catalog/origin/{{this._id}}"><button
                        class="btn btn-outline-primary btn-sm mt-3 mr-2">Wines</button></a>
                <a href="/catalog/origin/{{this._id}}/delete"><button
                        class="btn btn-outline-danger btn-sm mt-3">Delete</button></a>
            </li>

        </ul>

        {{/each}}
    </div>
</div>